# List for other links, not main
# Do not edit this file manually if you need add new links or remove unexisting.
# To modify links use addlink and removelink utilites
include	\usr\fidoconf\link.lst 

# 1-st point for you
# You may use .1 point when accesing from other PC from network 
Link SYSTEM-OPERATOR-NAME
Aka MYNODE-ADDRESS.1
ourAka MYNODE-ADDRESS
Password MYPOINT-PASSWORD
EchoMailFlavour Hold 
FileEchoFlavour Hold
Packer zip

# This is robot for posting statictics, annoncements and etc.
Link Dumb-robot 
Aka MYNODE-ADDRESS.333
ourAka MYNODE-ADDRESS
Packer zip

# List for points except .1 saved in \usr\fidoconf\point.lst file
# Do not edit this file manually
# To modify use addpoint and removepoint utilities
include	\usr\fidoconf\point.lst 

# File routing.lst contains direct routes to other links
# except of first and main link
# Do not add  routes to this file manually.
# To add or delete routes routes use addlink and removelink utilites
# to view current routes us 
include	\usr\fidoconf\routing.lst 

# last route for netmail of my node 
route immediate no-route MYNODE-ADDRESS.*
routefile immediate no-route MYNODE-ADDRESS.*

# Default route uplink saved in route-default.lst
# file.
# Other requests forward uplink link node 
# Do not modify this file manually
# If you need change default route
# use uplinkset utility
include	\usr\fidoconf\route-default.lst

